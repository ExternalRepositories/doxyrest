        <location file="/home/vladimir/Develop/alsa-lib/alsa-lib-1.1.3/include/seq_midi_event.h" line="54" column="1" bodyfile="/home/vladimir/Develop/alsa-lib/alsa-lib-1.1.3/src/seq/seq_midi_event.c" bodystart="398" bodyend="461"/>
