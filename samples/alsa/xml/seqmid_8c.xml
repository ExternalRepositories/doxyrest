        <definition>int snd_seq_control_queue</definition>
        <argsstring>(snd_seq_t *seq, int q, int type, int value, snd_seq_event_t *ev)</argsstring>
        <name>snd_seq_control_queue</name>
        <param>
          <type><ref refid="group___sequencer_1gae141149958a7458806152ed5942a1acd" kindref="member">snd_seq_t</ref> *</type>
          <declname>seq</declname>
